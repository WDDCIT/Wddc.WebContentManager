            $("#edit-form #site").kendoRadioGroup({
                items: [{
                    value: -1,
                    label: "All"
                }, {
                    value: 1,
                    label: "AB"
                }, {
                    value: 2,
                    label: "MB"
                }, {
                    value: 3,
                    label: "BC"

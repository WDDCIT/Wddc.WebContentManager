        function cancelUpdate() {

            var spinner = startSpinner('spinningWheelEdit');

            const element = $('#headingOne');
            $('html, body').animate({
                scrollTop: $(element).offset().top
            }, 900);

            stopSpinner(spinner, 'spinningWheelEdit');
            document.getElementById("message-toUpdate-row").style.visibility = "hidden";
        }

        function cancelDelete() {

            var spinner = startSpinner('spinningWheelDelete');

            const element = $('#headingTwo');
            $('html, body').animate({
                scrollTop: $(element).offset().top
            }, 900);

            stopSpinner(spinner, 'spinningWheelDelete');
            document.getElementById("message-toDelete-row").style.visibility = "hidden";
        }


        function prepareLogList() {
            setupPagination('log', 'log-list', '@Url.Action("Log", "GenericMessage")?referrerUrl=GenericMessage', ['Log', 'Username', 'Created'], 7);
        }

        function startSpinner(targetId) {
            var opts = {
                lines: 30, // The number of lines to draw
                length: 5, // The length of each line
                width: 10, // The line thickness
                radius: 20, // The radius of the inner circle
                scale: 1, // Scales overall size of the spinner
                corners: 1, // Corner roundness (0..1)
                color: '#A0A0A0',
                fadeColor: 'transparent', // CSS color or array of colors
                speed: 1, // Rounds per second
                rotate: 0, // The rotation offset
                animation: 'spinner-line-fade-quick', // The CSS animation name for the lines
                direction: 1, // 1: clockwise, -1: counterclockwise
                zIndex: 2e9, // The z-index (defaults to 2000000000)
                className: 'spinner', // The CSS class to assign to the spinner
                top: '50%', // Top position relative to parent
                left: '50%', // Left position relative to parent
                shadow: '0 0 1px transparent', // Box-shadow for the lines
                position: 'absolute' // Element positioning
                //position: 'fixed' // Element positioning
            };

            var target = document.getElementById(targetId);
            var spinner = new Spinner(opts).spin(target);
            return spinner;
        }

        function stopSpinner(spinner, targetId) {
            spinner.stop();
        }


        var $myGroup = $('#accordion');
        $myGroup.on('show.bs.collapse','.collapse', function() {
            $myGroup.find('.collapse.in').collapse('hide');
        });

        $("#edit-form #messageId").kendoDropDownList({
            dataSource: {
                transport: {
                    read: {
                        url: '@Url.Action("GetMessages", "GenericMessage")',
                        dataType: "json"
                    }
                }
            },
            dataTextField: "Subject",
            dataValueField: "MsgGenericID",
            filter: "contains",
            open: function (e) {
                document.getElementById("message-toUpdate-row").style.visibility = "hidden";
            }
        });

        $("#delete-form #messageId").kendoDropDownList({
            dataSource: {
                transport: {
                    read: {
                        url: '@Url.Action("GetMessages", "GenericMessage")',
                        dataType: "json"
                    }
                }
            },
            dataTextField: "Subject",
            dataValueField: "MsgGenericID",
            filter: "contains",
            open: function (e) {
                document.getElementById("message-toDelete-row").style.visibility = "hidden";
            }
        });


        function submitCreateForm() {

            startSpinner('spinningWheelCreate'); 
            $('#create-form-submit').attr('disabled', true);
            $('#create-form #priority').val($('#create-form #priority').data("kendoSwitch").value());
        }

        function submitEditForm() {

            startSpinner('spinningWheelEdit'); 
            $('#edit-form-submit').attr('disabled', true);
            $('#edit-form #priority').val($('#edit-form #priority').data("kendoSwitch").value());
        }

        function submitDeleteForm() {

            var spinner = startSpinner('spinningWheelDelete');
            $('#delete-form-submit').attr('disabled', true);

            var confirmDelete = confirm("Confirm deleting generic message?");

            if (confirmDelete == true) {
                return true; 
            }
            else {
                stopSpinner(spinner, "spinningWheelDelete");
                $('#delete-form-submit').attr('disabled', false);
                return false;
            }
        }

        function searchMessageToUpdate() {

            var messageId = $("#edit-form #messageId").data("kendoDropDownList").value();
            
            var spinner = startSpinner("spinningWheelSearchUpdate");

            document.getElementById("message-toUpdate-row").style.visibility = "hidden";

            $.get('@Url.Action("GetMessageById", "GenericMessage")?Id=' + messageId, (result) => {

                document.getElementById("update-form-textarea-container").innerHTML = "<textarea id='subject' name='subject' style='width: 100%;' required></textarea>";

                $("#edit-form #subject").kendoTextArea({
                    maxLength: 128,
                    value: result.Subject
                });

                $("#edit-form #pdf-view-current-filename").html(result.FileName);

                $("#edit-form #pdf-view-current").attr("href", "Message_Generic_Temp\\" + result.FileName);

                $("#edit-form #expiryDate").data("kendoDatePicker").value(result.ExpiryDate);

                if (result.Location == 0)
                    $("#edit-form #site").getKendoRadioGroup().value(-1);
                else
                    $("#edit-form #site").getKendoRadioGroup().value(result.Location);

                if (result.Priority == 1)
                    $("#edit-form #priority").data("kendoSwitch").check(true);
                else
                    $("#edit-form #priority").data("kendoSwitch").check(false);

                document.getElementById("message-toUpdate-row").style.visibility = "visible";

                const element = $('#headingTwo');
                $('html, body').animate({
                    scrollTop: $(element).offset().top
                }, 900);

                stopSpinner(spinner, "spinningWheelSearchUpdate");
            });
        }

        function searchMessageToDelete() {

            var messageId = $("#delete-form #messageId").data("kendoDropDownList").value();

            var spinner = startSpinner("spinningWheelSearchDelete");

            document.getElementById("message-toDelete-row").style.visibility = "hidden";

            $.get('@Url.Action("GetMessageById", "GenericMessage")?Id=' + messageId, (result) => {

                document.getElementById("delete-form-textarea-container").innerHTML = "<textarea id='subject' name='subject' style='width: 100%;'></textarea>";

                $("#delete-form #subject").kendoTextArea({
                    maxLength: 128,
                    value: result.Subject,
                    enable: false
                });

                $("#delete-form #pdf-view-current-filename").html(result.FileName);

                $("#delete-form #pdf-view-current").attr("href", "Message_Generic_Temp\\" + result.FileName);

                $("#delete-form #expiryDate").data("kendoDatePicker").value(result.ExpiryDate);
                $("#delete-form #expiryDate").data("kendoDatePicker").enable(false);

                if (result.Location == 0)
                    $("#delete-form #site").getKendoRadioGroup().value(-1);
                else
                    $("#delete-form #site").getKendoRadioGroup().value(result.Location);

                $("#delete-form #site").getKendoRadioGroup().enable(false);

                if (result.Priority == 1)
                    $("#delete-form #priority").data("kendoSwitch").check(true);
                else
                    $("#delete-form #priority").data("kendoSwitch").check(false);

                $("#delete-form #priority").data("kendoSwitch").enable(false);

                document.getElementById("message-toDelete-row").style.visibility = "visible";

                const element = $('#headingThree');
                $('html, body').animate({
                    scrollTop: $(element).offset().top
                }, 900);

                stopSpinner(spinner, "spinningWheelSearchDelete");
            });
        }


    </script>
